{
  "solidity.packageDefaultDependenciesDirectory": "packages/hardhat-ts/node_modules",
  "solidity-va.test.defaultUnittestTemplate": "hardhat",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/.cache": true,
    "**/.vs/": true,
    "**/*.cs": true,
    "**/*.orig": true,
    "**/bin/": true,
    "**/build/": true,
    "**/debug/": true,
    "**/dist/": true,
    "**/node_modules/": true,
    "**/obj": true,
    "yarn-error.log": true,
    "**/yarn-error.log": true,
    "packages\\eth-hooks/lib": true
  },
  "explorerExclude.backup": null,
  "eslint.workingDirectories": [
    { "directory": "packages/eth-hooks", "changeProcessCWD": true },
    { "directory": "packages/hardhat-ts", "changeProcessCWD": true },
    { "directory": "packages/vite-app-ts", "changeProcessCWD": true }
  ],
  "search.exclude": {
    "**/yarn-error.log": true,
    "**/yarn.lock": true
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#cc60a7",
    "activityBar.activeBorder": "#afd16e",
    "activityBar.background": "#cc60a7",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#afd16e",
    "activityBarBadge.foreground": "#15202b",
    "panel.border": "#cc60a7",
    "sash.hoverBorder": "#cc60a7",
    "sideBar.border": "#cc60a7",
    "statusBar.background": "#bd3c91",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#cc60a7",
    "statusBarItem.remoteBackground": "#bd3c91",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "tab.activeBorder": "#cc60a7"
  },
  "peacock.color": "#bd3c91",
  "solidity.defaultCompiler": "localNodeModule"
}
